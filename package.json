{
  "name": "ai-agents-exploration",
  "version": "1.0.0",
  "description": "Multi-framework AI agents exploration with accountancy specialization",
  "main": "index.js",
  "scripts": {
    "dev": "npm run start:claude && npm run start:monitoring",
    "start:claude": "node frameworks/claude-code/mcp-server.js",
    "start:monitoring": "node utils/monitoring/dashboard.js",
    "test": "jest",
    "test:agents": "jest agents/",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "build": "npm run build:claude && npm run build:monitoring",
    "build:claude": "tsc frameworks/claude-code/src/**/*.ts --outDir frameworks/claude-code/dist",
    "build:monitoring": "webpack utils/monitoring/src/index.ts",
    "docs": "typedoc --out docs/api frameworks/",
    "format": "prettier --write .",
    "clean": "rimraf dist/ coverage/ .nyc_output/",
    "setup": "npm install && npm run setup:python",
    "setup:python": "pip install -r requirements.txt"
  },
  "keywords": [
    "ai",
    "agents",
    "claude-code",
    "microsoft-ai",
    "langchain",
    "accountancy",
    "automation",
    "mcp"
  ],
  "author": "Nouri",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "inquirer": "^12.9.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/ai-agents-exploration.git"
  },
  "bugs": {
    "url": "https://github.com/username/ai-agents-exploration/issues"
  },
  "homepage": "https://github.com/username/ai-agents-exploration#readme"
}
